{
  "version": 2,
  "builds": [
    {
      "src": "api/terminal.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/terminal.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ],
  "functions": {
    "api/terminal.py": {
      "runtime": "python3.9"
    }
  },
  "outputDirectory": "public"
}